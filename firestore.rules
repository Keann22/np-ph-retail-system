rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    // WARNING: These rules are insecure and are intended as a temporary measure
    // to resolve persistent permission errors. They allow any authenticated
    // user to read and write all data.
    match /{document=**} {
      allow read, write: if request.auth != null;
    }
  }
}
